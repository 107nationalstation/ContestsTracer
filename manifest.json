{
	"name" : "ContestsTracer",
	"version" : "0.1",
	"manifest_version" : 2,
	"icons" : {
		"48" : "icon.png"

	},
	"description" : "抓取最近的比赛",
	"browser_action" : {
		"default_icon" : "icon.png",
		"default_popup" : "popup.html"
	},
	"background" : {
		"scripts" : ["background.js"]
	},

	"permissions" : ["notifications","http://contests.acmicpc.info/","alarms", "tabs", "bookmarks", "declarativeWebRequest", "*://*/*"]
}
